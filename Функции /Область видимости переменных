x = 10  # Глобальная переменная
y = 20  # Глобальная переменная
def change_value():
    global y # Сделали переменную внутри фун-ции глобальной
    x = 20  # Локальная переменная (не влияет на глобальную)
    y = 30
    print(x, y)  # 20

change_value()
print(x, y)  # 10 (глобальная x не изменилась, а y присвоенно новое значение)
