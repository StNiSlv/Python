x = 27 # Присвоем переменной х число 27
print(x, type(x)) # Выведем переменную x, и ее обозначение
print(str(x), type(str(x))) # Изменим тип данных на строку
print(x*x) # Умножение чисел
print(x/x) # Деление чисел (при деление числа типа int на int получается число типа float)
print(x+x) # Сложение 
print(x.__sizeof__()) # Узнаем размер в байтах 
